pipeline {
  agent any
  stages {
    stage('Anan') {
      parallel {
        stage('Anan') {
          steps {
            sleep 5
          }
        }
        stage('Anan2') {
          steps {
            echo 'zzzz'
          }
        }
      }
    }
    stage('Aditya') {
      steps {
        echo 'Hello'
      }
    }
  }
}